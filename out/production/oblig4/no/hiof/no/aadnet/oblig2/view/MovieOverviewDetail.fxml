      </VBox>
      <Label layoutX="7.0" layoutY="48.0" prefHeight="21.0" prefWidth="84.0" text="Title:" />
      <Label layoutX="7.0" layoutY="69.0" prefHeight="21.0" prefWidth="84.0" text="Release:" />
      <Label layoutX="7.0" layoutY="101.0" prefHeight="21.0" prefWidth="84.0" text="Duration:" />
      <Label layoutX="7.0" layoutY="139.0" prefHeight="21.0" prefWidth="84.0" text="Description" />
   </children>
</AnchorPane>
